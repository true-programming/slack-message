{
    "name": "trueprogramming/slack-message",
    "description": "Adds slack monolog implementation",
    "type": "typo3-cms-extension",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^12.0",
        "monolog/monolog": "^3"
    },
    "autoload": {
        "psr-4": {
            "Trueprogramming\\SlackMessage\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "slack_message"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "typo3/coding-standards": "^0.8.0",
        "saschaegerer/phpstan-typo3": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
